    "query = '''\n",
    "SELECT title\n",
    "FROM film_text\n",
    "WHERE film_id IN (\n",
    "    SELECT film_id \n",
    "    FROM inventory\n",
    "    WHERE   inventory_id IN (\n",
    "        SELECT inventory_id\n",
    "        FROM rental\n",
    "        WHERE customer_id =(\n",
    "            SELECT customer_id\n",
    "            FROM payment\n",
    "            GROUP BY customer_id\n",
    "            ORDER BY sum(amount) DESC \n",
    "            LIMIT 1)));\n",
    "'''\n",
